@model ShopASP.Models.Context.ModelsForDB.Person

<div conteiner-person>
    <h2>Personal information</h2>
    <div class="conteiner-inner">
        <div class="conteiner_div">
            <p>Surname</p>
            <p>@Model.Surname</p>
        </div>

        <div class="conteiner_div">
            <p>Firstname</p>
            <p>@Model.Firstname</p>
        </div>

    </div>
    <div class="conteiner_div">
        <p>Patronymic</p>
        <p>@Model.Patronymic</p>
    </div>

    <h2>Contact information</h2>
    <div class="conteiner-inner">
        <div class="conteiner_div">
            <p>Phone:</p>
            <p>@Model.Phone</p>
        </div>
        <div class="conteiner_div">
            <p>Email:</p>
            <p>@Model.Email</p>
        </div>

</div>
